<html>
	<head>
		<title>Home - NotaryChain Client</title>
		<script>
			function tree(data) {
		    	var string
			    if (typeof(data) == "object") {
			    	string = "<ul>";
			        for (var i in data) {
			        	string += "<li>" + i + tree(data[i]);
			        }
			        string += "</ul>";
			    } else {
			    	string = " => " + data;
			    }
		    	return string
			}
		</script>
	</head>
	<body>
		<body onload='entry.innerHTML = tree({{.}})'>
		<h1>Home</h1>
		<p>Edit Entry:</p>
		<ul>
			<!-- <li><a href="/form">By Form</a></li> -->
			<li><a href="/json">As JSON</a></li>
		</ul>
		<h3>Entry:</h3>
		<div id="entry"></div>
		<form method="post" action="http://localhost:8083/">
			<input type="hidden" name="format" value="json" />
			<input type="hidden" name="data" value="{{.}}" />
			<input type="submit" name="submit" value="Save" />
		</form>
	</body>
</html>